
#model
score.math %>%
  tam(.) -> tam.mod1



#we need gender as values not df
# replace with dataset name and gender variable name
gender2<-gold.data[,"ST004Q01_15"] #1 female, 2 male


#Set up facet variable
formulaA2 <- ~item+item:step+item*gender2
facets2 <- as.data.frame(gender2)

score.math %>%
  tam.mml.mfr( ., facets= facets2 , formulaA = formulaA2 )->a



a[[5]]%>% as.data.frame(.) -> tam.mod32
